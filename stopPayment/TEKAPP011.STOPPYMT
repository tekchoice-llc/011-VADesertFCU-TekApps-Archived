  DO
    HtmlVars(StopAmount,HtmlFieldValue)="0.00"
    HtmlFieldDisabled=TRUE
  END
 IF HtmlVars(StopAmount,HtmlFieldValue)="" THEN HtmlVars(StopAmount,HtmlFieldValue)="0.00"
