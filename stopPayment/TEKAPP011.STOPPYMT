 HtmlTxtFieldName=StopCheckNumberEnd
 CALL TekProCreateLabelField
 HTMLVIEWLINE("<br/>")
 HtmlTxtFieldName=StopAmount
 CALL TekProCreateLabelField
 HTMLVIEWLINE("<br/>")
 HtmlTxtFieldName=StopPayableTo
